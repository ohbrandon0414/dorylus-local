#! /bin/bash

cd $( dirname $0 )/..

# First argument is the idea of the node running the weight server
weightserverNode=${1}

if [ ! -d ./results ]; then
	mkdir results
fi


# Args: <Coordserver-Port> <Weightserver-IP> <Weightserver-Port> <Dataserver-Port>
./build/coordination_server 55430 $( ./utils/ec2 pubip ) 55432 55431
